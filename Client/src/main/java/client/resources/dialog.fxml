<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="100.0" prefWidth="300.0"
      xmlns="http://javafx.com/javafx/10.0.2-internal" fx:controller="client.Controller">
    <TextField fx:id="folderName" promptText="Folder name">
        <VBox.margin>
            <Insets left="5.0" right="5.0" top="10.0"/>
        </VBox.margin>
    </TextField>
    <Button text="Submit" cancelButton="true" mnemonicParsing="false" onAction="#submitCreateFolder">
        <VBox.margin>
            <Insets bottom="5.0" left="5.0"/>
        </VBox.margin>
    </Button>
</VBox>